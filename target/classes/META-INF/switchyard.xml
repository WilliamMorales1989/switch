<?xml version="1.0" encoding="UTF-8"?>
<sy:switchyard xmlns:sy="urn:switchyard-config:switchyard:1.1" xmlns:bean="urn:switchyard-component-bean:config:1.1" xmlns:camel="urn:switchyard-component-camel:config:1.1" xmlns:jpa="urn:switchyard-component-camel-jpa:config:1.1" xmlns:sca="http://docs.oasis-open.org/ns/opencsa/sca/200912" xmlns:soap="urn:switchyard-component-soap:config:1.1" xmlns:transform="urn:switchyard-config:transform:1.1" name="switch-aprovisionamiento" targetNamespace="urn:com.ec.tvcable.switchaprov:switch-aprovisionamiento:1.0">
    <sca:composite name="switch-aprovisionamiento" targetNamespace="urn:com.ec.tvcable.switchaprov:switch-aprovisionamiento:1.0">
        <sca:service name="Aprovisionamiento" promote="AprovisionamientoBean/Aprovisionamiento">
            <sca:interface.wsdl interface="wsdl/Aprovisionamiento.wsdl#wsdl.porttype(Aprovisionamiento)"/>
            <soap:binding.soap name="soap1">
                <soap:wsdl>wsdl/Aprovisionamiento.wsdl</soap:wsdl>
                <soap:wsdlPort>AprovisionamientoSOAP</soap:wsdlPort>
                <soap:contextPath>switch-aprovisionamiento</soap:contextPath>
            </soap:binding.soap>
        </sca:service>
        <sca:reference multiplicity="0..1" name="WsdlTvPagada" promote="TvInterfaceService/WsdlTvPagada">
            <sca:interface.wsdl interface="wsdl/WsdlTvPagada.wsdl#wsdl.porttype(WsdlTvPagada)"/>
            <soap:binding.soap name="soap1">
                <soap:wsdl>wsdl/WsdlTvPagada.wsdl</soap:wsdl>
                <soap:endpointAddress>http://pablo-a6200:8088/mockWsdlTvPagadaHttpBinding</soap:endpointAddress>
            </soap:binding.soap>
        </sca:reference>
        <sca:reference multiplicity="0..1" name="WsdlIntraway" promote="IntrawayInterfaceService/WsdlIntraway">
            <sca:interface.wsdl interface="wsdl/WsdlIntraway.wsdl#wsdl.porttype(WsdlIntraway)"/>
            <soap:binding.soap name="soap1">
                <soap:wsdl>wsdl/WsdlIntraway.wsdl</soap:wsdl>
                <soap:endpointAddress>http://pablo-a6200:8088/mockWsdlIntrawayHttpBinding</soap:endpointAddress>
            </soap:binding.soap>
        </sca:reference>
