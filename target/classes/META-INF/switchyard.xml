            <sca:interface.java interface="com.ec.tvcable.switchaprov.TransactionSpResponseService"/>
            <jpa:binding.jpa>
                <jpa:entityClassName>com.ec.tvcable.switchaprov.jpa.TransactionSpResponse</jpa:entityClassName>
                <jpa:persistenceUnit>switch-aprovisionamiento</jpa:persistenceUnit>
                <jpa:transactionManager>#jtaTransactionManager</jpa:transactionManager>
                <jpa:produce>
                    <jpa:flushOnSend>true</jpa:flushOnSend>
                    <jpa:usePersist>true</jpa:usePersist>
                </jpa:produce>
            </jpa:binding.jpa>
        </sca:reference>
        <sca:component name="TvInterfaceService">
            <bean:implementation.bean class="com.ec.tvcable.switchaprov.impl.TvInterfazServiceBean"/>
            <sca:service name="TvInterfaceService">
                <sca:interface.java interface="com.ec.tvcable.switchaprov.TvInterfaceService"/>
            </sca:service>
            <sca:reference name="WsdlTvPagada">
                <sca:interface.java interface="com.ec.tvcable.switchaprov.service.tvpagada.WsdlTvPagada"/>
            </sca:reference>
            <sca:reference name="DatosTvPagada">
                <sca:interface.java interface="com.ec.tvcable.switchaprov.DatosTvPagada"/>
            </sca:reference>
        </sca:component>
        <sca:component name="IntrawayInterfaceService">
            <bean:implementation.bean class="com.ec.tvcable.switchaprov.impl.IntrawayInterfaceServiceBean"/>
