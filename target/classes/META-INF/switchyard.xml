        <transform:transform.jaxb contextPath="com.ec.tvcable.switchaprov.service.aprov" from="{http://www.example.org/Aprovisionamiento/}Aprovisionamiento" to="java:com.ec.tvcable.switchaprov.service.aprov.Aprovisionamiento_Type"/>
    </sy:transforms>
</sy:switchyard>
