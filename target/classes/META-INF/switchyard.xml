            <sca:reference name="WsdlIntraway">
                <sca:interface.java interface="com.ec.tvcable.switchaprov.service.intraway.WsdlIntraway"/>
            </sca:reference>
        </sca:component>
        <sca:component name="InterfazResolverBean">
            <bean:implementation.bean class="com.ec.tvcable.switchaprov.impl.InterfazResolverBean"/>
            <sca:service name="InterfazResolver">
                <sca:interface.java interface="com.ec.tvcable.switchaprov.InterfazResolver"/>
            </sca:service>
            <sca:reference name="InterfazAprovServicio">
                <sca:interface.java interface="com.ec.tvcable.switchaprov.InterfazAprovServicio"/>
            </sca:reference>
        </sca:component>
        <sca:component name="InterfazAprovServicioBean">
            <bean:implementation.bean class="com.ec.tvcable.switchaprov.impl.InterfazAprovServicioBean"/>
            <sca:service name="InterfazAprovServicio">
                <sca:interface.java interface="com.ec.tvcable.switchaprov.InterfazAprovServicio"/>
            </sca:service>
        </sca:component>
        <sca:component name="AprovisionamientoBean">
            <bean:implementation.bean class="com.ec.tvcable.switchaprov.AprovisionamientoBean"/>
            <sca:service name="Aprovisionamiento">
                <sca:interface.java interface="com.ec.tvcable.switchaprov.service.aprov.Aprovisionamiento"/>
            </sca:service>
            <sca:reference name="InterfazResolver">
                <sca:interface.java interface="com.ec.tvcable.switchaprov.InterfazResolver"/>
            </sca:reference>
            <sca:reference name="IntrawayInterfaceService">
                <sca:interface.java interface="com.ec.tvcable.switchaprov.IntrawayInterfaceService"/>
            </sca:reference>
            <sca:reference name="TvInterfaceService">
                <sca:interface.java interface="com.ec.tvcable.switchaprov.TvInterfaceService"/>
            </sca:reference>
            <sca:reference name="TransactionSpResponseService">
                <sca:interface.java interface="com.ec.tvcable.switchaprov.TransactionSpResponseService"/>
            </sca:reference>
        </sca:component>
        <sca:component name="DatosTvPagadaBean">
            <bean:implementation.bean class="com.ec.tvcable.switchaprov.impl.DatosTvPagadaBean"/>
            <sca:service name="DatosTvPagada">
                <sca:interface.java interface="com.ec.tvcable.switchaprov.DatosTvPagada"/>
            </sca:service>
        </sca:component>
    </sca:composite>
    <sy:transforms>
        <transform:transform.jaxb contextPath="com.ec.tvcable.switchaprov.service.tvpagada" from="java:com.ec.tvcable.switchaprov.service.tvpagada.Comando" to="{http://www.grupotvcable.com.ec/WsdlTvPagada/}ejecutaComando"/>
        <transform:transform.jaxb contextPath="com.ec.tvcable.switchaprov.service.tvpagada" from="{http://www.grupotvcable.com.ec/WsdlTvPagada/}consultaRespuesta" to="java:com.ec.tvcable.switchaprov.service.tvpagada.Respuesta"/>
        <transform:transform.jaxb contextPath="com.ec.tvcable.switchaprov.service.intraway" from="java:com.ec.tvcable.switchaprov.service.intraway.Comando" to="{http://www.grupotvcable.com.ec/WsdlIntraway/}ejecutaComando"/>
        <transform:transform.jaxb contextPath="com.ec.tvcable.switchaprov.service.intraway" from="{http://www.grupotvcable.com.ec/WsdlIntraway/}consultaRespuesta" to="java:com.ec.tvcable.switchaprov.service.intraway.Respuesta"/>
        <transform:transform.jaxb contextPath="com.ec.tvcable.switchaprov.service.ems" from="java:com.ec.tvcable.switchaprov.service.ems.EMSResponse" to="{http://www.example.org/EMS/}EMSResponse"/>
        <transform:transform.jaxb contextPath="com.ec.tvcable.switchaprov.service.ems" from="{http://www.example.org/EMS/}EMS" to="java:com.ec.tvcable.switchaprov.service.ems.EMS_Type"/>
