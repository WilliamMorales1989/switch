            		<xsd:element name="Interfaces" type="tns:Interface"
            			maxOccurs="unbounded" minOccurs="1">
            		</xsd:element>
            		<xsd:element name="SerialNumber"
            			type="xsd:string">
            		</xsd:element>
            		<xsd:element name="ErrorCode" type="xsd:int"></xsd:element>
            	</xsd:sequence>
            </xsd:complexType>
    
            <xsd:complexType name="Interface">
            	<xsd:sequence>
            		<xsd:element name="InterfazId" type="xsd:int"></xsd:element>
            		<xsd:element name="ErrorCode" type="xsd:int"></xsd:element>
